{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "no-unused-expression": false,
    "object-literal-sort-keys": false,
    "no-empty-interface": true,
    "no-console": [true, "log", "error"]
  },
  "rulesDirectory": []
}
